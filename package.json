  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=6400",
    "build": "ng build --prod",
    "test": "ng test --watch false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "manifest-create": "appcache-manifest \"dist/**/*.{html,css,js,ico,jpg,png,eot,woff2,woff,ttf}\" -o dist/cache.manifest --postfile postfile.manifest",
    "manifest-fix": "node build/add-manifest",
    "manifest": "npm run manifest-create && npm run manifest-fix",
    "ci": "npm run lint && npm run test && npm run build && npm run manifest"
  },
  "private": true,
